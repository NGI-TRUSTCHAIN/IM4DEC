#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/expo@49.0.6_@babel+core@7.22.9/node_modules/expo/bin/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/expo@49.0.6_@babel+core@7.22.9/node_modules/expo/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/expo@49.0.6_@babel+core@7.22.9/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/expo@49.0.6_@babel+core@7.22.9/node_modules/expo/bin/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/expo@49.0.6_@babel+core@7.22.9/node_modules/expo/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/expo@49.0.6_@babel+core@7.22.9/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/expo@49.0.6_@babel+core@7.22.9/node_modules/expo/bin/cli" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/expo@49.0.6_@babel+core@7.22.9/node_modules/expo/bin/cli" "$@"
fi
