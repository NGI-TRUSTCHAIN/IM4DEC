#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/@craco+craco@7.1.0_@types+node@20.8.2_postcss@8.4.32_react-scripts@5.0.1_typescript@5.2.2/node_modules/@craco/craco/dist/bin/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/@craco+craco@7.1.0_@types+node@20.8.2_postcss@8.4.32_react-scripts@5.0.1_typescript@5.2.2/node_modules/@craco/craco/dist/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/@craco+craco@7.1.0_@types+node@20.8.2_postcss@8.4.32_react-scripts@5.0.1_typescript@5.2.2/node_modules/@craco/craco/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/@craco+craco@7.1.0_@types+node@20.8.2_postcss@8.4.32_react-scripts@5.0.1_typescript@5.2.2/node_modules/@craco/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/@craco+craco@7.1.0_@types+node@20.8.2_postcss@8.4.32_react-scripts@5.0.1_typescript@5.2.2/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/@craco+craco@7.1.0_@types+node@20.8.2_postcss@8.4.32_react-scripts@5.0.1_typescript@5.2.2/node_modules/@craco/craco/dist/bin/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/@craco+craco@7.1.0_@types+node@20.8.2_postcss@8.4.32_react-scripts@5.0.1_typescript@5.2.2/node_modules/@craco/craco/dist/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/@craco+craco@7.1.0_@types+node@20.8.2_postcss@8.4.32_react-scripts@5.0.1_typescript@5.2.2/node_modules/@craco/craco/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/@craco+craco@7.1.0_@types+node@20.8.2_postcss@8.4.32_react-scripts@5.0.1_typescript@5.2.2/node_modules/@craco/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/@craco+craco@7.1.0_@types+node@20.8.2_postcss@8.4.32_react-scripts@5.0.1_typescript@5.2.2/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@craco/craco/dist/bin/craco.js" "$@"
else
  exec node  "$basedir/../@craco/craco/dist/bin/craco.js" "$@"
fi
