#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/typeorm@0.3.17_pg@8.11.2_sqlite3@5.1.6_ts-node@10.9.1/node_modules/typeorm/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/typeorm@0.3.17_pg@8.11.2_sqlite3@5.1.6_ts-node@10.9.1/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/typeorm@0.3.17_pg@8.11.2_sqlite3@5.1.6_ts-node@10.9.1/node_modules/typeorm/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/typeorm@0.3.17_pg@8.11.2_sqlite3@5.1.6_ts-node@10.9.1/node_modules:/Users/christoph/projects/im4dec/impl/veramo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli.js" "$@"
else
  exec node  "$basedir/../typeorm/cli.js" "$@"
fi
